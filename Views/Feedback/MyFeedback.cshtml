            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
           
            @* <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.CourseId })  
                @Html.ActionLink("Details", "Details", new {id= item.CourseId  })  
                @Html.ActionLink("Add Material", "Addmaterial", new { id=item.CourseId })
            </td> *@
        </tr>
}
